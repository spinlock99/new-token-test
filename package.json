  "scripts": {
    "dash": "webpack-dashboard -- yarn run atomic-apps",
    "start": "concurrently --prefix \"[{name}]\" --names \"ATOMIC-APPS,GITHUB,SOCKETIO\" -c \"magenta,cyan,blue\" \"yarn run atomic-apps\" \"yarn run github\" \"yarn run socketio\"",
    "build": "rm -r bin/ && yarn run webpack:prod",
    "postbuild": "git add .; yarn version --new-version patch; git push",
    "atomic-apps": "node atomic-apps.js",
    "github": "node workers/github.js",
    "github-debug": "node debug workers/github.js",
    "github-webhooks": "node workers/github-webhooks.js",
    "socketio": "node socket-io.js",
    "webpack:dev": "FILESTACK_API_KEY=$FILESTACK_API_KEY webpack-dev-server --env=dev --inline --hot",
    "webpack:prod": "FILESTACK_API_KEY=$FILESTACK_API_KEY webpack --env=prod"
  }
